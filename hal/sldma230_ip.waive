//-----------------------------------------------------------------------------
// Abstract : HAL Wavier file for SLDMA230
//-----------------------------------------------------------------------------

lint_checking designunit = sldma230
{
    // Clock and Reset Naming drops SYS and CORE prefix in Instantiated Modules
    DIFCLK {"SYS_|CORE_"} off;
    
    // Glue Logic when linting as top-level
    ATLGLC off;
    
    // Expeting Registers on outputs when linting as top-level
    TPOUNR off;
    
    // DMA Active Signal Not Wired in
    UNCONN {"dma_active"} off;
    UNCONO {"dma_active"} off;
    
}

